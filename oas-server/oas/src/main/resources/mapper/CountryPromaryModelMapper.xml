<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cascv.oas.server.timezone.mapper.CountryPromaryModelMapper">
<resultMap id="countryPromaryModelMap" type="com.cascv.oas.server.timezone.model.CountryPromaryModel">
	<id column="uuid" property="uuid" />
	<result column="time_zone" property="timeZone"/>
	<result column="country_name" property="countryName"/>
	<result column="promary_name" property="promaryName"/>
  </resultMap>
  
  <sql id="Base_Column_List">
    uuid, time_zone, country_name,promary_name
  </sql>
  
  <select id="selectTimeZone" resultMap="countryPromaryModelMap">
  select 
  		time_zone 
  from 
  		country_promary 
  where
  		promary_name=#{name} or country_name=#{name};
  </select>
  
