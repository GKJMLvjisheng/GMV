<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cascv.oas.server.energy.mapper.EnergySourcePowerMapper">
  <resultMap id="BaseResultMap" type="com.cascv.oas.server.energy.model.EnergySourcePower">
    <result column="id" jdbcType="INTEGER" property="id" />
    <result column="source_code" jdbcType="INTEGER" property="sourceCode" />
    <result column="source_name" jdbcType="VARCHAR" property="sourceName" />
    <result column="type" jdbcType="VARCHAR" property="type" />
    <result column="power_single" jdbcType="DECIMAL" property="powerSingle" />
    <result column="power_total" jdbcType="DECIMAL" property="powerTotal" />
    <result column="power_increase_speed" jdbcType="DECIMAL" property="powerIncreaseSpeed" />
    <result column="power_each_ball" jdbcType="DECIMAL" property="powerEachBall" />
  </resultMap>
  <select id="queryPowerSingle" resultMap="BaseResultMap">
    select power_single from energy_source_power where source_code = 1
  </select>
</mapper>