<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cascv.oas.server.energy.mapper.EnergySourcePointMapper">
    <resultMap id="BaseResultMap" type="com.cascv.oas.server.energy.model.EnergySourcePoint">
        <id column="uuid" jdbcType="VARCHAR" property="uuid"/>
        <result column="source_code" jdbcType="INTEGER" property="sourceCode"/>
        <result column="source_name" jdbcType="VARCHAR" property="sourceName"/>
        <result column="type" jdbcType="VARCHAR" property="type"/>
        <result column="point_single" jdbcType="DECIMAL" property="pointSingle"/>
        <result column="point_total" jdbcType="DECIMAL" property="pointTotal"/>
        <result column="point_increase_speed" jdbcType="DECIMAL" property="pointIncreaseSpeed"/>
        <result column="point_increase_speed_unit_name" jdbcType="VARCHAR" property="pointIncreaseSpeedUnitName"/>
        <result column="point_increase_speed_unit_value" jdbcType="DECIMAL" property="pointIncreaseSpeedUnitValue"/>
        <result column="point_max_each_ball" jdbcType="DECIMAL" property="pointMaxEachBall"/>
    </resultMap>

  <select id="queryPointSingle" resultMap="BaseResultMap">
    select point_single from energy_source_point where source_code = 1
  </select>
</mapper>